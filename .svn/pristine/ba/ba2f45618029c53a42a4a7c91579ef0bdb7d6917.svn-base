<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
 <display-name>TeamProject</display-name>
  <servlet>
    <servlet-name>ManageUserServlet</servlet-name>
    <servlet-class>com.sys.servlet.ManageUserServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>CommodityListServlet</servlet-name>
    <servlet-class>com.sys.index.servlet.CommodityListServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>CommodityDetailServlet</servlet-name>
    <servlet-class>ccom.sys.index.servlet.CommodityDetailServlet</servlet-class>
  </servlet>
  
  
  <servlet>
    <servlet-name>ManageOrderServlet</servlet-name>
    <servlet-class>com.sys.servlet.ManageOrderServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManageCategoreServlet</servlet-name>
    <servlet-class>com.sys.servlet.ManageCategoreServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManageComServlet</servlet-name>
    <servlet-class>com.sys.servlet.ManageComServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManageLoginServlet</servlet-name>
    <servlet-class>com.sys.servlet.ManageLoginServlet</servlet-class>
  </servlet>


  <servlet>
    <servlet-name>ManageCommentServlet</servlet-name>
    <servlet-class>com.sys.servlet.ManageCommentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserLoginServlet</servlet-name>
    <servlet-class>com.sys.index.servlet.UserLoginServlet</servlet-class>
  </servlet>



  <filter>
    <filter-name>UnicodeFilter</filter-name>
    <filter-class>com.sys.filter.UnicodeFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>UnicodeFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping> 



  <servlet-mapping>
    <servlet-name>CommodityListServlet</servlet-name>
    <url-pattern>/CommodityListServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CommodityDetailServlet</servlet-name>
    <url-pattern>/CommodityDetailServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ManageUserServlet</servlet-name>
    <url-pattern>/ManageUserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageOrderServlet</servlet-name>
    <url-pattern>/ManageOrderServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageCategoreServlet</servlet-name>
    <url-pattern>/ManageCategoreServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageComServlet</servlet-name>
    <url-pattern>/ManageComServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageLoginServlet</servlet-name>
    <url-pattern>/ManageLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageCommentServlet</servlet-name>
    <url-pattern>/ManageCommentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserLoginServlet</servlet-name>
    <url-pattern>/UserLoginServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>